.cell{
    padding-left:30rpx;
}

.title{
    color: #3b445b;
    font-size: 30rpx;
    margin-bottom:15rpx;
}

.comment{
    color: #959baa;
    font-size: 24rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow:hidden;
    width:63%;
}
.date{
    color: #b4bacc;
    font-size: 24rpx;
    float: right;
}
.mark{
    height: 16rpx;
    width: 16rpx;
    border-radius: 10rpx;
    background: red;
    display: inline-block;
}

.bgView{
    width: 100%;
    height: auto;
    margin-bottom:100rpx;
    border-top:2rpx solid #edeff2;
}

.button{
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 98rpx;
    border-top:2rpx solid #edeff2;
    color: #ff5600;
    line-height: 98rpx;
    text-align: center;
    font-size:32rpx;
    background: #fff
}