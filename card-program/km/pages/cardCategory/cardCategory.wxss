.search{
  width: 92%;
  background: #ffffff;
  margin: 12rpx 30rpx;
  position:relative;
}
.searchInput{
  height: 60rpx;
  line-height:60rpx;
  font-size: 28rpx;
  color: #959baa;
  text-align:left;
  padding-left:80rpx;
  background: #f9fbfe;
  border-radius:30rpx;
}
.searchImg{
  width: 30rpx;
  height: 30rpx;
  position:absolute;
  left: 25rpx;
  top: 15rpx;
  z-index:1;
  margin:0 5rpx;
  padding:0 5rpx;
}